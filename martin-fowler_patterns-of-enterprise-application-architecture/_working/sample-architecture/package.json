{
  "name": "sample-architecture",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "env-cmd node serve.js",
    "dev:products:migrate": "env-cmd sequelize-cli db:migrate --config ./databases/products_db/config.cjs --migrations-path ./databases/products_db/migrations",
    "dev:products:migrate-undo": "env-cmd sequelize-cli db:migrate:undo:all --config ./databases/products_db/config.cjs --migrations-path ./databases/products_db/migrations",
    "dev:products:seed": "env-cmd sequelize-cli db:seed:all --config ./databases/products_db/config.cjs --seeders-path ./databases/products_db/seeders",
    "lint": "eslint .",
    "start": "node serve.js",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mysql2": "^1.7.0",
    "sequelize": "^5.21.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "faker": "^4.1.0",
    "jest": "^26.0.1",
    "sequelize-cli": "^5.5.1"
  }
}
